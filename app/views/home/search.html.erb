<table class="tablehero">
  <thead> 
    <tr>
      <th><h1>Player id</h1></th>
      <th><h1>Name</h1></th>
      <th><h1>URL</h1></th>
    </tr>
  </thead>
  <tbody>
    <% @all_usernames["results"].each do |res| %>
    <tr>
      <td><a><%=  link_to "#{res["player_id"]}", "/username/#{res["player_id"]}" %></a></td>
      <td><%= res["name"] %></td>
      <td><%= res['career_url'] %></td>
    </tr>
    <% end %>
  </tbody>
</table>
